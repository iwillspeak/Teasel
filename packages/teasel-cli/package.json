{
  "name": "@iwillspeak/teasel-cli",
  "version": "0.3.0",
  "type": "module",
  "description": "Command Line Interface to Teasel HTML Parser",
  "bin": "./bin/teasel-cli",
  "main": "lib/main.js",
  "preferGlobal": true,
  "files": [
    "lib/!(test)",
    "bin/*"
  ],
  "types": "lib/main.d.ts",
  "scripts": {
    "clean": "rimraf ./lib",
    "prebuild": "npm run clean",
    "build": "tsc",
    "start": "node lib/main.js",
    "lint": "eslint \"src/**/*.ts\"",
    "prepublishOnly": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iwillspeak/Teasel.git"
  },
  "author": "Will Speak <will@willspeak.me>",
  "contributors": [
    "James Garbutt"
  ],
  "license": "MIT",
  "keywords": [
    "html",
    "parser",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/iwillspeak/Teasel/issues"
  },
  "homepage": "https://github.com/iwillspeak/Teasel#readme",
  "dependencies": {
    "@iwillspeak/teasel": "^0.3.0",
    "@types/yargs": "^17.0.10",
    "yargs": "^17.4.1"
  }
}