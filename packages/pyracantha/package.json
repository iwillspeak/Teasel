{
  "name": "@iwillspeak/pyracantha",
  "version": "0.3.0",
  "type": "module",
  "description": "Red/Green syntax trees for Typescript",
  "main": "lib/main.js",
  "files": [
    "lib/!(test)"
  ],
  "types": "lib/main.d.ts",
  "scripts": {
    "test": "c8 --reporter=text-summary mocha \"lib/test/**/*_test.js\"",
    "clean": "rimraf ./lib",
    "prebuild": "npm run clean",
    "build": "tsc",
    "lint": "eslint \"src/**/*.ts\"",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iwillspeak/Teasel.git"
  },
  "author": "Will Speak <will@willspeak.me>",
  "license": "MIT",
  "keywords": [
    "syntax",
    "trees",
    "red-green"
  ],
  "bugs": {
    "url": "https://github.com/iwillspeak/Teasel/issues"
  },
  "homepage": "https://github.com/iwillspeak/Teasel#readme"
}