{
  "name": "parser",
  "version": "0.0.1",
  "description": "A parser of some sort.",
  "main": "lib/main.js",
  "files": [
    "lib/!(test)"
  ],
  "scripts": {
    "clean": "rimraf ./lib",
    "lint": "eslint \"src/**/*.ts\"",
    "prebuild": "npm run clean",
    "build": "tsc",
    "test": "c8 --reporter=text-summary mocha \"lib/test/**/*_test.js\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iwillspeak/a-n-parser.git"
  },
  "author": "Parser Authors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iwillspeak/a-n-parser/issues"
  },
  "homepage": "https://github.com/iwillspeak/a-n-parser#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-google": "^0.14.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  }
}
